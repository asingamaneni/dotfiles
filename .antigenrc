antigen bundles <<EOBUNDLES
	colored-man-pages
	command-not-found
	docker
	git-extras
	gradle
	httpie
	pip
	thefuck
	zsh-users/zsh-completions
	zsh-users/zsh-history-substring-search
	zsh-users/zsh-syntax-highlighting
	zsh-users/zsh-autosuggestions
	StackExchange/blackbox
	lukechilds/zsh-nvm
	lukechilds/zsh-better-npm-completion
EOBUNDLES

SPACESHIP_PROMPT_ORDER=(time user host dir git aws package node ruby elixir xcode swift golang php rust haskell julia venv conda dotnet ember docker kubecontext exec_time line_sep battery vi_mode jobs exit_code char)
SPACESHIP_PROMPT_DEFAULT_PREFIX='using '
SPACESHIP_TIME_SHOW=true
SPACESHIP_TIME_FORMAT='%D{%I:%H:%S %p, %a %d %b %Y}'
SPACESHIP_USER_PREFIX='as ' 
SPACESHIP_USER_COLOR=green
SPACESHIP_HOST_PREFIX='on '
SPACESHIP_HOST_COLOR=cyan
SPACESHIP_DIR_COLOR=red
SPACESHIP_GIT_BRANCH_COLOR=blue
SPACESHIP_VENV_SYMBOL=🐍
SPACESHIP_VENV_COLOR=green
SPACESHIP_PYENV_PREFIX='with '
SPACESHIP_PYENV_COLOR=green
SPACESHIP_KUBECONTEXT_PREFIX='using '
SPACESHIP_EXEC_TIME_COLOR=blue

antigen theme https://github.com/denysdovhan/spaceship-prompt spaceship

antigen apply

